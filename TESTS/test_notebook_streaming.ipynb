    "# Code based on C code streaming_C_DLL_example2.c\n",
    "\n",
    "import sys\n",
    "import time\n",
    "import numpy as np\n",
    "from random import random\n",
    "\n",
    "# ATTOCUBE lib\n",
    "import ATTOCUBE.IDS as IDS\n",
    "import ATTOCUBE.streaming.stream as ids_stream \n",
    "\n",
    "# Param saved into a config file\n",
    "IDS3010ADDRESS = \"172.27.36.217\"\n",
    "BUFFERSIZE = 2048 \n",
    "\n",
    "\n",
    "isMaster = True \n",
    "intervalInMicroseconds = 100 # To be selected in a combo\n",
    "kwargs_stream = {\"filePath\": None, \n",
    "                 \"axis0\": False, \"axis1\": True, \"axis2\": False\n",
    "                }\n",
    "bytesInBuffer = 0\n",
    "samplesToReceive = 50;   \n",
    "\n",
    "sampleBuffersSize = samplesToReceive + 1022\n",
    "\n",
    "# Instantiation and connexion\n",
    "ids = IDS.Device(IDS3010ADDRESS)\n",
    "ids.connect()\n",
    "current_mode = ids.system.getCurrentMode()\n",
    "\n",
    "if current_mode == \"system idle\":\n",
    "    ids.system.startMeasurement()\n",
